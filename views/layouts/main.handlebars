    <script src='https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML'></script>
  {{/if}}

  <script src="/js/materialize.js"></script>
  <script src="/js/init.js"></script>

  {{#if includeAnalytics }}
    {{>analytics}}
  {{/if}}

  </body>
</html>
